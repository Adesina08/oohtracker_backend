# Example environment variables for Snack Track Insight App
# Copy this file to .env.local and fill in your actual values

# Frontend variables
VITE_JWT_SECRET=your_jwt_secret_here
VITE_API_BASE_URL=http://localhost:4001

# Backend variables
AZURE_TEXT_ANALYTICS_ENDPOINT=your_text_analytics_endpoint_here
AZURE_TEXT_ANALYTICS_KEY=your_text_analytics_key_here
DB_HOST=localhost
DB_PORT=5432
DB_USER=snackuser
DB_PASSWORD=snackpass
DB_NAME=snacktrack
AZURE_STORAGE_CONNECTION_STRING=your_storage_connection_string_here
AZURE_AUDIO_CONTAINER=your_audio_container_name
AZURE_MEDIA_CONTAINER=your_media_container_name
